{
  "name": "@tokenring-ai/drizzle-storage",
  "version": "0.2.0",
  "description": "Multi-database storage for Token Ring using Drizzle ORM",
  "dependencies": {
    "@tokenring-ai/app": "0.2.0",
    "@tokenring-ai/chat": "0.2.0",
    "@tokenring-ai/checkpoint": "0.2.0",
    "zod": "catalog:",
    "drizzle-orm": "^0.45.1",
    "mysql2": "^3.15.3",
    "postgres": "^3.4.7"
  },
  "devDependencies": {
    "drizzle-kit": "^0.31.8",
    "vitest": "^4.0.15",
    "testcontainers": "^11.10.0",
    "bun-types": "catalog:"
  },
  "exports": {
    ".": "./index.ts",
    "./*": "./*.ts"
  },
  "license": "MIT",
  "scripts": {
    "test": "vitest",
    "db:generate": "bun run db:generate:sqlite && bun run db:generate:postgres && bun run db:generate:mysql",
    "db:generate:sqlite": "cd sqlite && drizzle-kit generate",
    "db:generate:postgres": "cd postgres && drizzle-kit generate",
    "db:generate:mysql": "cd mysql && drizzle-kit generate"
  },
  "type": "module"
}
